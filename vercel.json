{
  "builds": [
    {
      "src": "myproject/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "myproject.settings",
    "PYTHON_VERSION": "5.1.1"  // or any other version you're using
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/myproject/wsgi.py"
    }
  ]
}
