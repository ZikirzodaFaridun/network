{
    "version": 2,
    "builds": [
      {
        "src": "myproject/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "env": {
      "DJANGO_SECRET_KEY": "@django-secret-key",
      "DB_NAME": "@db-name",
      "DB_USER": "@db-user",
      "DB_PASSWORD": "@db-password",
      "DB_HOST": "@db-host"
    }
  }
  
